<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="5" parallel="classes">
	<listeners>
		<listener class-name="utils.config.TestListener" />
	</listeners>

 
<test name="REGRESSION">
  		<parameter name="browser" value="chrome.local" />
		<parameter name="autoLogBug" value="false" />
		<classes>
			<!--Merge Leads -->
			<class name="LinkingCucumber.MergedLead.Merged_leads_from_the_same_company_email"></class>		
			<class name="LinkingCucumber.MergedLead.Merged_leads_from_the_same_public_email"></class> 
			<class name="LinkingCucumber.MergedLead.Merged_leads_from_different_emails_but_same_company"></class>
			<class name="LinkingCucumber.MergedLead.Merged_leads_from_different_emails_but_same_partner"></class>
			<!--NOT Merge Leads -->
			<class name="LinkingCucumber.NOTMergedLead.NOT_Merged_leads_from_the_different_public_email"></class> 	
			<class name="LinkingCucumber.NOTMergedLead.NOT_Merged_leads_from_the_different_emails_different_companies"></class>
			<class name="LinkingCucumber.NOTMergedLead.NOT_Merged_leads_from_the_different_emails_Not_belong_existing_partner"></class> 
			<class name="LinkingCucumber.NOTMergedLead.NOT_Merged_leads_FromIBleadsPartnerSignUpNULL"></class> 
			<class name="LinkingCucumber.NOTMergedLead.NOT_Merged_leads_IfOneLeadAssignedToInstallBaseTeam"></class>	

		</classes>
	</test>

</suite>